<Window x:Class="ZOmicronInfo.WindowLog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZOmicronInfo" Closing="Window_Closing" Width="310" Height="300">
    <Grid>
        <TextBox x:Name="txtbxLog" Canvas.Left="10" TextWrapping="Wrap" Canvas.Top="10" IsReadOnly="True" IsReadOnlyCaretVisible="True" Loaded="txtbxLog_Loaded"/>
    </Grid>
</Window>
